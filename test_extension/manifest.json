{
    "manifest_version": 2,
    "name": "Test Extension",
    "version": "1.0.0",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [{
      "matches": ["*://*.stackoverflow.com/*"],
      "js": ["content.mjs"]
    }],
    "web_accessible_resources":["script.mjs", "background.mjs"], 
    "type" : "module"
  }