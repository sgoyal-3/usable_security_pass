{
    "manifest_version": 2,
    "name": "Test Extension",
    "version": "1.0.0",
    "content_scripts": [{
      "matches": ["*://*.stackoverflow.com/*"],
      "js": ["content/content.mjs"]
    }],
    "permissions": [
      "tabs", "activeTab"
    ],
    "browser_action": {
      "default_popup" : "html/popup.html"
    },
    "background" : {
      "scripts": ["background/background.js"],
      "persistent" : false
    },
    "externally_connectable" : {
      "ids" : ["bebffpohmffmkmbmanhdpepoineaegai"],
      "matches" : ["*://*.stackoverflow.com/*"],
      "accepts_tls_channel_id" : false
    },
    "web_accessible_resources":["content/script.mjs", 
      "html/modal.html", "content/popup.js", "content/register.js"]
  }